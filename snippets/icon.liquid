{%- capture cl -%}
  m-icon m-icon--{{ name }} 
    {%- if size == 'extra-small' -%}
        m-svg-icon--small
    {%- elsif size == 'small' -%}
        m-svg-icon
    {%- elsif size == 'medium-small' -%}
        m-svg-icon--medium-small
    {%- elsif size == 'medium' -%}
        m-svg-icon--medium
    {%- elsif size == 'custom' -%}
        {{ class | strip }}
    {%- else -%}
        m-svg-icon--large
    {%- endif -%}
{%- endcapture -%}
